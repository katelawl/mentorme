<p id="notice"><%= notice %></p>

<h1>Listing Mentor Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mentor_profiles.each do |mentor_profile| %>
      <tr>
        <td><%= link_to 'Show', mentor_profile %></td>
        <td><%= link_to 'Edit', edit_mentor_profile_path(mentor_profile) %></td>
        <td><%= link_to 'Destroy', mentor_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mentor profile', new_mentor_profile_path %>
